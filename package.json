{
  "name": "school-registration-system",
  "version": "1.0.0",
  "description": "A school system for registering students,staff and parents",
  "main": "src/index.ts",
  "author": "McCarthy <mccarthymutebi10@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "nodemon --watch ./src --ext ts,js --exec \"yarn run tsx\" ",
    "tsx": "tsx ./src/index.ts",
    "test": "cross-env TZ=UTC jest --config jest.config.js --verbose false --passWithNoTests --color",
    "typescript": "tsc",
    "postinstall": "husky install",
    "prepare": "husky"
  },
  "dependencies": {
    "express": "^4.18.3",
    "mongodb": "^6.7.0",
    "mongoose": "^8.3.4"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.1-nightly.20240914",
    "@swc/core": "^1.7.39",
    "@swc/jest": "^0.2.36",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.27",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "tsx": "^4.7.2",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": "eslint --cache --fix --max-warnings=0",
    "*.{js,ts}": "prettier --write --list-different"
  },
  "packageManager": "yarn@4.3.0"
